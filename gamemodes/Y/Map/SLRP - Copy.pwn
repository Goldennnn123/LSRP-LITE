				return true;
			}
			else {
				SendClientMessage(playerid, COLOR_ADM, "SERVER: ไม่พร้อมใช้งาน.");
				return true;
			}
		}
	}
	SendClientMessage(playerid, COLOR_ADM, "SERVER: คุณไม่สามารถทำสิ่งนี้ได้ในตอนนี้.");
	return true;
}

