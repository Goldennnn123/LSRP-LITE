public IRC_OnConnect(botid, ip[], port)
{
	printf("*** IRC_OnConnect: Bot ID %d connected to %s:%d", botid, ip, port);

	if(botid == botAdmin)
	{
		IRC_JoinChannel(botid, IRC_CHANNEL, "()*&&&!special1245");
		IRC_AddToGroup(groupID, botid);
	}
	if(botid == botReports)
	{
		IRC_JoinChannel(botid, IRC_CHANNEL_EX, "gmsonly&9992");
		IRC_AddToGroup(groupReport, botid);
	}
	if(botid == botAnti)
	{
		IRC_JoinChannel(botid, IRC_CHANNEL_ANTI, "admsonly&&*)234");
		IRC_AddToGroup(groupAnti, botid);
	}
	if(botid == botLeads)
	{
		IRC_JoinChannel(botid, IRC_CHANNEL_LEADS, "leadadminscentral199500");
		IRC_AddToGroup(groupLeads, botid);
	}
	return 1;
}

public IRC_OnDisconnect(botid, ip[], port, reason[])
{
	printf("*** IRC_OnDisconnect: Bot ID %d disconnected from %s:%d (%s)", botid, ip, port, reason);
	// Remove the bot from the group
	IRC_RemoveFromGroup(groupID, botid);
	return 1;
}

public IRC_OnConnectAttempt(botid, ip[], port)
{
	printf("*** IRC_OnConnectAttempt: Bot ID %d attempting to connect to %s:%d...", botid, ip, port);
	return 1;
}

public IRC_OnConnectAttemptFail(botid, ip[], port, reason[])
{
	printf("*** IRC_OnConnectAttemptFail: Bot ID %d failed to connect to %s:%d (%s)", botid, ip, port, reason);
	return 1;
}

stock MySQLConnection(){

	m_Handle = mysql_connect(SQL_HOSTNAME, SQL_USERNAME, SQL_DATABASE, SQL_PASSWORD);

	if(m_Handle){

		printf("[MYSQL]: Connection to '%s' successful!", SQL_DATABASE);
	}
	else
	{
		printf("[MYSQL]: Connection to '%s' failed!", SQL_DATABASE);
		SendRconCommand("exit");
	}
}

public OnGameModeExit()
{
	//Saving:
	SaveProperties();
	SaveFactions();
	SaveBusinesses();
	SaveTolls();
	SavePaynSprays();
	SaveDoors();
	SavePropertyGarages();

	//Closing database:
	mysql_close(m_Handle);

	//IRC:
	IRC_Quit(botAdmin, "The server has shut down.");
	IRC_Quit(botReports, "The server was shut down.");
	IRC_Quit(botAnti, "The server was shut down.");
	IRC_Quit(botLeads, "The server was shut down.");

	IRC_DestroyGroup(groupID);
	IRC_DestroyGroup(groupReport);
	IRC_DestroyGroup(groupAnti);
	IRC_DestroyGroup(groupLeads);

	/*
	Profiler_Stop();
	Profiler_Dump();*/
	return true;
}

