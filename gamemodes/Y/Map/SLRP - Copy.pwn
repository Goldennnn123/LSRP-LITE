			new
				vehicleid = GetPlayerVehicleID(playerid)
			;

			if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER)
				return SendClientMessage(playerid, COLOR_YELLOW, "คุณไม่ได้นั่งอยู่ที่คนขับ");

			if(IsValidFactionCar(vehicleid) && PlayerData[playerid][pFaction] != FactionCar[vehicleid][carFaction])
				return SendClientMessage(playerid, COLOR_YELLOW, "SERVER: คุณไม่ได้รับอนุญาติให้เข้าถึงรถคันนี้");

			GetVehicleParamsEx(vehicleid, engine, lights, alarm, doors, bonnet, boot, objective);

