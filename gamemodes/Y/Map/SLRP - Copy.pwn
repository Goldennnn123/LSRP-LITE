CMD:ad(playerid, params[])
{
	if(!IsPlayerInRangeOfPoint(playerid, 5.0, AD_CENTER_X, AD_CENTER_Y, AD_CENTER_Z))
		return SendClientMessage(playerid, COLOR_ADM, "SERVER: คุณไม่ได้อยู่ที่ประกาศ.");

	if(PlayerData[playerid][pDonator] != 3)
	{
		switch(PlayerData[playerid][pDonator])
		{
			case 2:
			{
				if(gettime() - PlaceAdWait[playerid] < 5)
					return SendClientMessage(playerid, COLOR_ADM, "SERVER: คุณต้องรอ 5 วินาทีระหว่างโฆษณา.");
			}
		}
	}
	else
	{
		if(gettime() - PlaceAdWait[playerid] < 30)
			return SendClientMessage(playerid, COLOR_ADM, "SERVER: คุณต้องรถ 5 วินาทีระหว่างโฆษณา");
	}

	if(isnull(params) || strlen(params) < 3)
		return SendClientMessage(playerid, COLOR_ADM, "USAGE: /ad [advertisement]");

	if(150 > PlayerData[playerid][pMoney])
		return SendClientMessage(playerid, COLOR_ADM, "คุณไม่สามารถจ่ายได้ ($150)");

	PlaceAdWait[playerid] = gettime();

	new
		str[128];

	if(strlen(params) > 80)
	{
		SendMessageToAll(COLOR_DARKGREEN, "[Advertisement] %.80s ...", params);
		SendMessageToAll(COLOR_DARKGREEN, "[Advertisement] ...%s, โทรศัพท์: %i", params[80], ReturnPhoneNumber(playerid));
	}
	else SendMessageToAll(COLOR_DARKGREEN, "[Advertisement] %s, โทรศัพท์: %i", params, ReturnPhoneNumber(playerid));

	GiveMoney(playerid, -150);

	format(str, sizeof(str), "โฆษณาโดย %s[ID: %i]", ReturnName(playerid), playerid);
	SendAdminMessage(COLOR_YELLOW, str);

	format(str, sizeof(str), "%s(%i) made an advertisement: %s",  ReturnName(playerid), playerid, params);
	IRC_GroupSay(groupAnti, IRC_CHANNEL_ANTI, str);
	return 1;
}

