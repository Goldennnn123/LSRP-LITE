		for(new i = 0; i < 2; i++)
			KillTimer(PlayerData[playerid][p911CallTimer][i]);

		return 1;
	}

	if(PlayerData[playerid][pPhoneline] == 789 || PlayerData[playerid][pPhoneline] == 788)
