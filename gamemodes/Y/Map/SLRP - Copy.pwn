	if(!IsPlayerInAnyVehicle(playerid))return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} You aren't in any vehicle.");

	new vehicleid = GetPlayerVehicleID(playerid), string[128];
