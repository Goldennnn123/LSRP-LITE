{
	if(cache_num_rows(m_Handle) != 0) //Exists.
	{
		ShowLoginDialog(playerid, "");
	}
	else //Doesn't exist.
	{
		ShowRegisterDialog(playerid, "");
	}
	return true;
}

Server:SQL_CheckPlayerBans(playerid)
{
	if(cache_num_rows() == 0)return CheckPlayerExist(playerid);

	new string[128], rows, fields;
	cache_get_data(rows, fields, m_Handle);

	if(rows){
		format(string, sizeof(string), "Your IP: %s is banned from our server.", ReturnIP(playerid));
		SendClientMessage(playerid, COLOR_YELLOW, string);
		SendClientMessage(playerid, COLOR_YELLOW, "Make a ban appeal on our forums.");

		format(string, sizeof(string), "%s [IP: %s] connected while banned and was kicked", ReturnName(playerid, 1), ReturnIP(playerid));
		adminWarn(1, string);

		KickEx(playerid);
	}

	return true;
}

