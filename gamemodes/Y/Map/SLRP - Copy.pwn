					new
						largeString[800];

					strcat(largeString, "{F81414}YOU'RE ABOUT TO CHANGE YOUR PASSWORD!\n");

					strcat(largeString, "{FFFFFF}Here are a few tips to keep your account password safe and secure:\n\n");
