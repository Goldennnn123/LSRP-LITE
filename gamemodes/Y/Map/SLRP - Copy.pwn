CMD:arrest1(playerid, params[])
{
	if(!IsPoliceFaction(playerid))
		return UnAuthMessage(playerid);

	if(!PlayerData[playerid][pLAWduty])
		return UnAuthMessage(playerid);
		

	new
		playerb, time,
		str[128];

	if(sscanf(params, "ui", playerb, time))
		return SendClientMessage(playerid, COLOR_ADM, "USAGE: /arrest1 [ไอดีผู้เล่น/ชื่อบางส่วน] [time]");

	if(!IsPlayerConnected(playerb))
		return SendClientMessage(playerid, COLOR_ADM, "ERROR: ไม่พบ ID ผู้เล่น.");

	if(!GetDistanceBetweenPlayers(playerid, playerb, 5) && !IsPlayerInDynamicArea(playerb, JailCenterArea))
		return SendClientMessage(playerid, COLOR_ADM, "ERROR: คุณต้องอยู่ใกล้ผู้เล้นมากกว่านี้.");
		
	SendClientMessage(playerid, COLOR_GREY, "คุณถูกเจ้าหน้าที่ %s จับกุม");

	PlayerData[playerb][pPhoneOff] = true;
	PlayerData[playerb][pICJailTime] = time * 60;
	PlayerData[playerb][pICJailed] = 1;
    SendPlayer(playerid, 227.4976, 109.8629, 999.0156, 0, 0);

    
	if(PlayerData[playerb][pActiveListing])
		PlayerData[playerb][pActiveListing] = 0;

	PlayerData[playerb][pJailTimes]++;

	new
		jail_str[90];

	TotalJailees++;

	format(jail_str, 90, "%03d", TotalJailees);
	PlayerData[playerb][pICJail3D] = CreateDynamic3DTextLabel(jail_str, 0x1E0C95FF, 0.0, 0.0, -0.10, 20.0, playerb);

	format(str, sizeof(str), "[Jail] %s %s  เพิ่งถูกจับกุม %s!", ReturnRank(playerid), ReturnName(playerid), ReturnName(playerb));
	SendLawMessage(COLOR_ADM, str);

	format(str, sizeof(str), "[Jail]  คุณถูกจับกุมโดย %s สําหรับ (%d) นาที!", ReturnName(playerid), time);
	SendClientMessage(playerb, COLOR_ADM, str);

	SavePlayer(playerb);
	return 1;
}
