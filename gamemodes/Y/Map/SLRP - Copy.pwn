public OnDialogResponse(playerid, dialogid, response, listitem, inputtext[])
{
	switch(dialogid)
	{
		case DIALOG_CONFIRM_SYS:{
			ConfirmDialog_Response(playerid, response);
		}
		case DIALOG_REGISTER:{

			if(!response) return Kick(playerid);
			
			if(LockedServer)
			{
				SendClientMessage(playerid, COLOR_ADM, "เซิฟเวอร์อยู่ในสถานะล็อคดาวน์โปรดลองอีกครั้งในภายหลัง");
 				KickEx(playerid);
	    		return 1;
	    	}

			if(strlen(inputtext) < 3 || strlen(inputtext) > 30){
				ShowRegisterDialog(playerid, "ความยาวรหัสผ่านของคุณควรสูงกว่า 3 ตัวอักษรและยาวไม่เกิน 30 ตัวอักษร");
				return true;
			}

			new
				thstndrd[10]
			;

			switch(CountRegister() + 1)
			{
				case 1,21,31: format(thstndrd, sizeof(thstndrd), "st");
				case 2,22: format(thstndrd, sizeof(thstndrd), "nd");
				case 3,23: format(thstndrd, sizeof(thstndrd), "rd");
				default: format(thstndrd, sizeof(thstndrd), "th");
			}

			SetPlayerCamera(playerid);
			format(PlayerPassword[playerid], 90, "%s", inputtext);

			Message(playerid, COLOR_GOLD, "Welcome to Street Life Roleplay, %s {FFFFFF}["REVISION"]", ReturnName(playerid));
			SendClientMessage(playerid, -1, " ");

			Message(playerid, COLOR_WHITE, "* คุณจะเป็นของเรา %i%s ผู้ใช้ที่ลงทะเบียน.", (CountRegister() + 1), thstndrd);
			SendClientMessage(playerid, COLOR_WHITE, "* ก่อนที่คุณจสมัครสมาชิกคุณจะต้องผ่านการแนะนำสั้น ๆ นี้ไปยังเซิร์ฟเวอร์ของเรา.");

			PlayerTutorialTimer[playerid] = SetTimerEx("StartPlayerTutorial", 3000, false, "i", playerid);
