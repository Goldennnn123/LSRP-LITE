		return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณไม่ได้อยู่ไนธนาคาร.");

	if(!PlayerData[playerid][pSavings]){
		if(sscanf(params, "d", amount))return SendClientMessage(playerid, COLOR_LIGHTRED, "การใช้: เพื่อสร้างบัญชีออมทรัพย์, /savings [จํานวนเงิน]");
		if(amount < 50000 || amount > 100000)return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณสามารถฟากเงินออมทรัพย์ได้เพียง $50,000 ถึง $100,000 .");
		if(amount > PlayerData[playerid][pMoney])return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณไม่มีเงินมากพอ.");

		PlayerData[playerid][pSavings] = amount;
		GiveMoney(playerid, -amount);

		SendClientMessage(playerid, COLOR_WHITE, "|___ BANK STATEMENT ___|");
		format(string, sizeof(string), "   Savings account balance: $%s", MoneyFormat(PlayerData[playerid][pSavings]));
		SendClientMessage(playerid, COLOR_WHITE, string);

		WriteLog("money_logs/savings_log.txt", "[%s] %s started their savings with : $%d", ReturnDate(), ReturnName(playerid, 1), amount);
		return true;
	}
	else if(isnull(params) && PlayerData[playerid][pSavings]){
		SendClientMessage(playerid, COLOR_WHITE, "|___ BANK STATEMENT ___|");
		format(string, sizeof(string), "   Savings account balance: $%s", MoneyFormat(PlayerData[playerid][pSavings]));
		SendClientMessage(playerid, COLOR_WHITE, string);

		format(string, sizeof(string), "[ ! ]{FFFFFF} คุณมี {FF6346}$%s{FFFFFF} ไนบัญชีออมทรัพย์.", MoneyFormat(PlayerData[playerid][pSavings]));
		SendClientMessage(playerid, COLOR_ADM, string);

		SendClientMessage(playerid, COLOR_LIGHTRED, "การใช้: เพื่อถอนการออมของคุณ, ไช้ {FF6346}/savings withdraw");
		return true;
	}

	if(!isnull(params) && !strcmp(params, "withdraw", true)){
		if (!PlayerData[playerid][pSavings])return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณไม่มีบัญชีออมทรัพย์.");

		ConfirmDialog(playerid, "Confirmation", "คุณแน่ใจหรือไม่ว่าต้องการที่จะถอนการออมของคุณ?\n\n{F81414}คุณไม่สามารถเปลี่ยนกลับได้และคุณจะต้องเริ่มต้นใหม่.", "OnPlayerSavingsWithdraw");

		return true;
	}

	return true;
}

