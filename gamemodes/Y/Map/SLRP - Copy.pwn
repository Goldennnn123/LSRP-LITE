							else{
								new
									query[128];

								mysql_format(m_Handle, query, sizeof(query), "อัพเดทผู้เล่น SET HasCarSpawned = 0 WHERE id = %i LIMIT 1", CarData[tmpcar][carOwnerID]);
								mysql_pquery(m_Handle, query);
