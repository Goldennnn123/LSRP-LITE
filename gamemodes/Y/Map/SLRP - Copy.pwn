		return true;
	}

	PlayerData[playerid][pSQLID] = cache_get_field_content_int(0, "id", m_Handle);
	PlayerData[playerid][pLogintime] = 0;

	GetPlayerIp(playerid, PlayerData[playerid][pIP], 20);

	LoadPlayerData(playerid);

	return true;
