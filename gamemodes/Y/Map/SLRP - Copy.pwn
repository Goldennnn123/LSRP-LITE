					adminWarn(1, str);
				}
				else
				{
					format(str, sizeof(str), "ผู้เล้น %s ถูก %d active OnAdminAction notes on him, last is: %s", ReturnName(playerb, 1), CountAdminNotes(playerb), aNotesData[playerb][i][anote_reason]);
