			mysql_format(m_Handle, query, sizeof(query), "INSERT INTO log_bans (Name, Reason, BannedBy, Date, IP) VALUES('%e', '%e', 'SYSTEM', '%e', '%e')", ReturnName(playerid, 1), reason, ReturnDateLog(), ReturnIP(playerid));
