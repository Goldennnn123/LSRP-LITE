CMD:hire(playerid, params[])
{
	if(CountPlayerBusinesses(playerid) == 0)
		return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} ึถณไม่ได้เป็นเจ้าของธุรกิจ.");

	if(!IsPlayerInBusiness(playerid))
		return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณไม่ได้อยู่ในธุรกิจของคุณ.");

	new
		bizid = IsPlayerInBusiness(playerid),
		playerb;

	if(BusinessData[bizid][businessOwnerSQL] != PlayerData[playerid][pSQLID])
		return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณไม่ได้เป็นเจ้าของธุรกิจ.");

	if(!isnull(params) && !strcmp(params, "cancel"))
	{
		if(OfferedHireTo[playerid] == INVALID_PLAYER_ID)
			return SendClientMessage(playerid, COLOR_ADM, "SERVER: คุณยังไม่ได้ยื่นข้อเสนอให้ใคร.");

		Message(OfferedHireTo[playerid], COLOR_ADM, "SERVER: %s cancelled their offer for you.", ReturnName(playerid));
		Message(playerid, COLOR_ADM, "SERVER: You cancelled your offer to %s.", ReturnName(OfferedHireTo[playerid]));

		OfferedHireBy[OfferedHireTo[playerid]] = INVALID_PLAYER_ID;
		OfferedHireTo[playerid] = INVALID_PLAYER_ID;
		OfferedHireBusinessID[playerid] = 0;
		return 1;
	}

	if(OfferedHireTo[playerid] != INVALID_PLAYER_ID)
		return SendClientMessage(playerid, COLOR_ADM, "SERVER: คุณมีข้อเสนอที่รอการดำเนินการรอให้พวกเขายอมรับหรือยกเลิก.");

	if(sscanf(params, "u", playerb))
		return SendClientMessage(playerid, COLOR_ADM, "USAGE: /hire [ไอดีผู้เล่น/ชื่อบางส่วน]");

	if(!IsPlayerConnected(playerb))
		return SendClientMessage(playerid, COLOR_ADM, "ERROR: คุณใส่ไอดีผู้เล่นผิด.");

	if(!GetDistanceBetweenPlayers(playerid, playerb, 5.0))
		return SendClientMessage(playerid, COLOR_ADM, "ERROR: คุณไม่ได้อยู่ไกล้ผู้เล่นคนนั้น.");

	if(OfferedHireBy[playerb] != INVALID_PLAYER_ID)
		return SendClientMessage(playerid, COLOR_ADM, "SERVER: ผู้เล่นคนนั้นมีข้อเสนออยู่แล้ว.");

	Message(playerid, COLOR_ADM, "You offered %s to work for %s.", ReturnName(playerb, 0), BusinessData[bizid][businessName]);
	SendClientMessage(playerid, COLOR_ADM, "ใช้ /hire cancel เพื่อยกเลิกข้อเสนอ.");

	Message(playerb, COLOR_ADM, "%s เสนองานให้คุณที่ %s. ใช้ /hireaccept เพื่อทำงานที่นั่น หรือ /hireaccept NO. เพื่อปฎิเสธข้อเสนอ", ReturnName(playerid, 0), BusinessData[bizid][businessName]);

	OfferedHireBusinessID[playerid] = BusinessData[bizid][businessID];

	OfferedHireBy[playerb] = playerid;
	OfferedHireTo[playerid] = playerb;
	return 1;
}

CMD:hireaccept(playerid, params[])
{
	if(OfferedHireBy[playerid] == INVALID_PLAYER_ID)
		return SendClientMessage(playerid, COLOR_ADM, "SERVER: ไม่มีใครเสนองานให้คุณ.");

	if(!strcmp(params, "yes", true))
	{

		Message(playerid, COLOR_ADM, "คุณยอมรับข้อเสนอของ %s's ตอนนี้คุณได้ทำงานที่ธุรกิจนี้แล้ว!", ReturnName(OfferedHireBy[playerid]));
		Message(OfferedHireBy[playerid], COLOR_ADM, "%s รับข้อเสนอของคุณ.", ReturnName(playerid));

		PlayerData[playerid][pWorkOn] = OfferedHireBusinessID[OfferedHireBy[playerid]];

		OfferedHireTo[OfferedHireBy[playerid]] = INVALID_PLAYER_ID;
		OfferedHireBusinessID[OfferedHireBy[playerid]] = 0;
		OfferedHireBy[playerid] = INVALID_PLAYER_ID;
	}
	else if(!strcmp(params, "no", true))
	{
		Message(playerid, COLOR_ADM, "คุณปฎิเสธข้อเสนอของ %s's.", ReturnName(OfferedHireBy[playerid]));
		Message(OfferedHireBy[playerid], COLOR_ADM, "%s ปฎิเสธข้อเสนอของคุณ.", ReturnName(playerid));

		OfferedHireTo[OfferedHireBy[playerid]] = INVALID_PLAYER_ID;
		OfferedHireBusinessID[OfferedHireBy[playerid]] = 0;
		OfferedHireBy[playerid] = INVALID_PLAYER_ID;
	}
	else return SendClientMessage(playerid, COLOR_ADM, "SERVER: It's either a YES or a NO.");
	return 1;
}

