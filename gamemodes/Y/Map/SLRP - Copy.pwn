CMD:handgun(playerid, params[])
{
	new	playerb, wepid;

	if(sscanf(params, "ui", playerb, wepid))
		return SendClientMessage(playerid, COLOR_ADM, "USAGE: /handgun [ไอดีผู้เล่น/ชื่อบางส่วน] [weapon id]");
		
 	if(IsPlayerInAnyVehicle(playerid))
		return SendClientMessage(playerid, COLOR_ADM, "ไม่.");

	if(!IsPlayerConnected(playerb))
		return SendClientMessage(playerid, COLOR_ADM, "ไม่มีผู้เล่นที่คุณระบุ.");

	if(!GetDistanceBetweenPlayers(playerid, playerb, 5.0))
		return SendClientMessage(playerid, COLOR_ADM, "คุณไม่ได้อยู่ไกล้ผู้เล่นคนนั้น.");

	if(playerb == playerid)
		return SendClientMessage(playerid, COLOR_ADM, "ไม่สามารถให้ปืนตัวเองได้");

	if(wepid <= 1 || wepid > 46 || wepid == 35 || wepid == 36 || wepid == 37 || wepid == 38 || wepid == 39 || wepid == 21 || wepid == 19)
	    return SendClientMessage(playerid, COLOR_GREY, "ระบุไอดีอาวุธผิด.");

	if(!PlayerHasWeapon(playerid, wepid))
		return SendClientMessage(playerid, COLOR_ADM, "คุณไม่มีอาวุธที่คุณระบุ.");

	new index = GetWeaponIndex(wepid);

	if(Weapon[playerb][index][eWeaponID] > 0)
		return SendClientMessage(playerid, COLOR_ADM, "ผู้เล่นนั้นมีอาวุธอยู่แล้วตามไอดีที่ระบุ.");

	WriteLog("weapon_logs/handgun.txt", "[%s] %s ให้ %s ของ %s ด้วย %d กระสุน", ReturnDateLog(), ReturnName(playerid), ReturnName(playerb), ReturnWeaponName(wepid), ReturnWeaponAmmo(playerid, wepid));

	Message(playerid, COLOR_ORANGE, "You handed your %s with %i Ammo to %s.", ReturnWeaponName(wepid), ReturnWeaponAmmo(playerid, wepid), ReturnName(playerb));
	Message(playerb, COLOR_ORANGE, "%s gave you their %s with %i Ammo.", ReturnName(playerid), ReturnWeaponName(wepid), ReturnWeaponAmmo(playerid, wepid));

	GivePlayerGun(playerb, wepid, ReturnWeaponAmmo(playerid, wepid));
	TakePlayerWeapon(playerid, wepid);
	return 1;
}

CMD:handammo(playerid, params[])
{
	new	playerb, ammo;

	if(sscanf(params, "ui", playerb, ammo))
	{
		SendClientMessage(playerid, COLOR_ADM, "USAGE: /handammo [ไอดีผู้เล่น/ชื่อบางส่วน] [ammo]");
		SendClientMessage(playerid, COLOR_ADM, "TIP:{FFFFFF} คุณส่งกระสุนจากปืนที่คุณถืออยู่.");
		return 1;
	}

	if(!IsPlayerConnected(playerb))
		return SendClientMessage(playerid, COLOR_ADM, "ไม่มีผู้เล่นที่ระบุ.");

	if(!GetDistanceBetweenPlayers(playerid, playerb, 5.0))
		return SendClientMessage(playerid, COLOR_ADM, "คุณไม่ได้อยู่ไกล้ผู้เล่นคนนั้น.");

	if(playerb == playerid)
		return SendClientMessage(playerid, COLOR_ADM, "คุณไม่สามารถให้กระสุนตัวเองได้.");

	if(GetPlayerWeapon(playerid) == 0)
		return SendClientMessage(playerid, COLOR_ADM, "ERROR: คุณไม่ได้ถืออาวุธอยู่.");

	new weapon_id = ReturnWeapon(playerid);
	new currAmmo = ReturnWeaponAmmo(playerid, weapon_id);

	if(weapon_id == -1)
		return SendClientMessage(playerid, COLOR_ADM, "ERROR: คุณไม่ได้ถืออาวุธ.");

	if(!PlayerHasWeapon(playerb, weapon_id))
		return SendClientMessage(playerid, COLOR_ADM, "ผู้เล่นคนนั้นไม่มีปืนที่คุณให้กระสนุไป.");

	if(ammo < 1 || ammo > currAmmo)
		return SendClientMessage(playerid, COLOR_ADM, "คุณไม่มีกระสุนจำนวนนั้น.");

	new bAmmo = ReturnWeaponAmmo(playerb, weapon_id);

	UpdatePlayerAmmo(playerb, weapon_id, bAmmo + ammo);
	SetPlayerAmmo(playerb, weapon_id, bAmmo + ammo);

	UpdatePlayerAmmo(playerid, weapon_id, currAmmo - ammo);
	SetPlayerAmmo(playerid, weapon_id, currAmmo - ammo);

	if(currAmmo - ammo <= 0)
	{
		TakePlayerWeapon(playerid, weapon_id);
	}

	Message(playerid, COLOR_ORANGE, "You gave %s ammo for their %s. (%i)", ReturnName(playerb), ReturnWeaponName(weapon_id), ammo);
	Message(playerb, COLOR_ORANGE, "%s gave you %i ammo for your %s.", ReturnName(playerid), ammo, ReturnWeaponName(weapon_id));

	WriteLog("weapon_logs/handammo.txt", "[%s] %s gave %s %i Ammo for a %s", ReturnDateLog(), ReturnName(playerid), ReturnName(playerb), ammo, ReturnWeaponName(weapon_id));
	return 1;
}

