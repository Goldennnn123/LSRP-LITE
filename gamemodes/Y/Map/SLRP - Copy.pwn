				}
			}
		}
	}

	new
		str[128];

	format(str, sizeof(str), "13**[Death] %s was killed by %s with %s.", ReturnName(playerid), ReturnName(killerid), ReturnWeaponName(weaponid));
	IRC_GroupSay(groupID, IRC_CHANNEL, str);

	WriteLog("player_logs/death_log.txt", "[%s] %s brutally wounded %s with a %s", ReturnDate(), ReturnName(killerid, 1), ReturnName(playerid, 1), ReturnWeaponName(weaponid));
