CMD:payfine(playerid, params[])
{
	new
		bool:hasFines = false;

	for(new i = 0; i < 30; i++)
	{
		if(!Fines[playerid][i][Fine_ID])
			continue;

		hasFines = true;
	}
	if(!hasFines) return SendClientMessage(playerid, COLOR_ADM, "SERVER: คุณไม่มีค่าปรับใดๆ.");

	new
		fine_id,
		bool:fine_exists = false,
		index,
		str[128];

	if(sscanf(params, "i", fine_id))
		return SendClientMessage(playerid, COLOR_GREY, "USAGE: /payfine [Fine ID] (สามารถดู Fine ID ได้ที่ /fines.)");

	for(new i = 0; i < 30; i++)
	{
		if(!Fines[playerid][i][Fine_ID])
			continue;

		if(Fines[playerid][i][Fine_ID] == fine_id)
		{
			fine_exists = true;
			index = i;
		}
	}
	if(!fine_exists) return SendClientMessage(playerid, COLOR_ADM, "SERVER: รหัส FINE ID ไม่ถูกต้อง.");


	format(str, sizeof(str), "คุณกำลังจะจ่าย {ADC3E7}Fine ID %i{FFFFFF} for {ADC3E7}$%s.", fine_id, MoneyFormat(Fines[playerid][index][Fine_Amount]));
	ConfirmDialog(playerid, "Confirmation", str, "OnPlayerPayFine", index);

	return 1;
}

