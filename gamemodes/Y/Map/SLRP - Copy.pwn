        if(FishCaught[playerid][1] >= 1 && FishCaught[playerid][2] >= 1 && FishCaught[playerid][3] >= 1 && FishCaught[playerid][4] >= 1 && FishCaught[playerid][5] >= 1) return SendClientMessage(playerid, COLOR_WHITE, "ปลาของคุณเต็มแล้ว!");
