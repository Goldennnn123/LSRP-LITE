Server:CallPaycheck()
{
	new string[128], taxCost, Float:interestGain, Paycheck;

	foreach(new i : Player){
		if(pLoggedIn[i]){

			new bank = PlayerData[i][pBank];
			new savings = PlayerData[i][pSavings];

			Paycheck = 0;
			taxCost = Paycheck / 10;

			new Float:save = 0.5;
			new Float:inter = 0.1;

			interestGain = (bank / 100) * inter;
			new Float:savingsGain = (savings / 100) * save;
			if(PlayerData[i][pLevel] >= 3) Paycheck+= 500;
			if(PlayerData[i][pLevel] == 1) Paycheck+= 1000;

			switch(PlayerData[i][pDonator])
			{
				case 2: Paycheck+= 1000;
				case 3: Paycheck+= 500;
				case 4: Paycheck+= 500;
			}

            
			new saveGain = floatround(savingsGain, floatround_round);
			new intGain = floatround(interestGain, floatround_round);

			if(PlayerData[i][pFaction] && FactionData[PlayerData[i][pFaction]][factionBank] > 0)
			{
				new factionPay;
				factionPay = FactionData[PlayerData[i][pFaction]][factionBank] / CountOnlineMembers(PlayerData[i][pFaction]);

				Paycheck+= factionPay;
				FactionData[PlayerData[i][pFaction]][factionBank]-= factionPay;
			}

			new carOwnerIG = 0;
			new carID_Found;
			for(new v = 1, j = GetVehiclePoolSize(); v <= j; v++)
			{
				if(!IsValidPlayerCar(v))
					continue;

				if(IsVehicleOccupied(v))
					continue;

				foreach(new c : Player)
				{
					if(PlayerData[c][pSQLID] == CarData[v][carOwnerID])
					{
						carOwnerIG = 1;
						carID_Found = v;
					}
				}
			}
			if(!carOwnerIG)
			{
				SaveCar(carID_Found);
				DestroyVehicle(carID_Found);
			}

			if(PlayerData[i][pLevel] == 1)
			{
				new exp = ((PlayerData[i][pLevel]) * 4 + 2);

				if(PlayerData[i][pExp] >= exp)
				{
					cmd_levelup(i, "");
				}
			}

			format(string, sizeof(string), "SERVER TIME:[ %s ]", ReturnHour());
			SendClientMessage(i, COLOR_WHITE, string);

			SendClientMessage(i, COLOR_WHITE, "|___ BANK STATEMENT ___|");
			format(string, sizeof(string), "   ยอดเงินในบัญชี: $%s", MoneyFormat(PlayerData[i][pBank]));
			SendClientMessage(i, COLOR_GREY, string);
			SendClientMessage(i, COLOR_GREY, "  อัตราดอกเบี้ย: 0.10");

			PlayerData[i][pPaycheck]+= Paycheck;
			PlayerData[i][pBank]-= PropertyData[PlayerData[i][pRenting]][propertyRentPrice];
			PlayerData[i][pBank]-= taxCost;
			PlayerData[i][pBank]+= intGain;

			if(PlayerData[i][pSavings] > 0){
				PlayerData[i][pSavings]+= saveGain;
			}

			format(string, sizeof(string), "   ดอกเบี้ยที่ได้รับ: $%s", MoneyFormat(intGain));
			SendClientMessage(i, COLOR_GREY, string);

			format(string, sizeof(string), "   จ่ายภาษีแล้ว: $%s", MoneyFormat(taxCost));
            SendClientMessage(i, COLOR_GREY, string);

			if(PlayerData[i][pSavings] > 0){
				format(string, sizeof(string), "   รายได้เงินฝากออมทรัพย์: $%s, อยู่ที่อัตรา: 0.5", MoneyFormat(saveGain));
				SendClientMessage(i, COLOR_GREY, string);

				format(string, sizeof(string), "   ยอดเงินในบัญชีออมทรัพย์ใหม่: $%s", MoneyFormat(PlayerData[i][pSavings]));
				SendClientMessage(i, COLOR_GREY, string);
			}

			SendClientMessage(i, COLOR_WHITE, "|________________________|");
			format(string, sizeof(string), "   ยอดเงินในบัญชี: $%s", MoneyFormat(PlayerData[i][pBank]));
		    SendClientMessage(i, COLOR_WHITE, string);
			format(string, sizeof(string), "   ค่าเช่า: -$%s", MoneyFormat(PropertyData[PlayerData[i][pRenting]][propertyRentPrice]));
			SendClientMessage(i, COLOR_WHITE, string);
			if (PlayerData[i][pLevel] == 1) {
				SendClientMessage(i, COLOR_WHITE, "(( คุณได้รับ $ 1,000 เนื่องจากคุณ level 1))");
			}
			format(string, sizeof(string), "~y~Payday~n~~w~Paycheck~n~~g~$%s", MoneyFormat(Paycheck));
		    GameTextForPlayer(i, string, 3000, 1);

			SavePlayer(i);
		}
	}
	return true;
}

