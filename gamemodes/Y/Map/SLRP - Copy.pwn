	GiveMoney(playerid, -3);

	PlayerData[playerid][pDrinks]++;
	return 1;
}

//Business commands
CMD:buybiz(playerid, params[])
{
	if(!pLoggedIn[playerid])return true;
	if(CountPlayerBusinesses(playerid) == 1)return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณเป็นเจ้าของธุรกิจแล้ว.");

	new string[128], bizid = 0;

	for(new i = 0; i < MAX_BUSINESS; i++){
		if(BusinessData[i][businessID] != 0){
			if(IsPlayerInRangeOfPoint(playerid, 3.0, BusinessData[i][businessExt][0], BusinessData[i][businessExt][1], BusinessData[i][businessExt][2])){
				bizid = i;
			}
		}
	}

	if(bizid == 0)return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณไม่ได้อยู่ไกล้ธุรกิจใดๆ.");
	if(BusinessData[bizid][businessOwnerSQL] != 0)return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณไม่สามารถซื้อกิจการที่มีเจ้าของอยู่แล้ว.");
	if(BusinessData[bizid][businessType] == 8 || BusinessData[bizid][businessType] == 9)return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณไม่สามารถซื้อธุรกิจประเภทนี้ได้.");
	if(PlayerData[playerid][pMoney] < BusinessData[bizid][businessMarketPrice]){
		format(string, sizeof(string), "จำนวนเงินไม่เพียงพอค่าธุรกิจนี้คือ $%s. โปรดเตรียมเงินให้พร้อมเพื่อชำระเงิน...", MoneyFormat(BusinessData[bizid][businessMarketPrice]));
		SendClientMessage(playerid, COLOR_ADM, string);
		return true;
	}
	if(PlayerData[playerid][pLevel] < BusinessData[bizid][businessLevel])return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} เลเวลคุณไม่พอที่จะซื้อธุรกิจนี้.");

	GiveMoney(playerid, -BusinessData[bizid][businessMarketPrice]);
	BusinessData[bizid][businessOwnerSQL] = PlayerData[playerid][pSQLID];

	PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
	GameTextForPlayer(playerid, "Congratulations!~n~You're now the owner of this business!", 4000, 5);

	SaveSQLInt(BusinessData[bizid][businessID], "businesses", "OwnerSQL", BusinessData[bizid][businessOwnerSQL]);

	DestroyDynamicPickup(BusinessData[bizid][businessPickup]);
	BusinessData[bizid][businessPickup] = CreateDynamicPickup(1239, 14, BusinessData[bizid][businessExt][0], BusinessData[bizid][businessExt][1], BusinessData[bizid][businessExt][2], 0);

	return true;
}

CMD:sellbiz(playerid, params[])
{
	if(!pLoggedIn[playerid])return true;
	if(CountPlayerBusinesses(playerid) == 0)return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณไม่ได้เป็นเจ้าของธุรกิจนี้.");

	new string[128], confirm[60];

	if(sscanf(params, "s[60]", confirm))return SendClientMessage(playerid, COLOR_ADM, "SERVER:{FFFFFF} คุณกำลังจะขายธุรกิจของคุณ. พิมพ์ '/sellbiz confirm' เพื่อยืนยันการขายธุรกิจของคุณ");

	if(strmatch(confirm, "confirm")){
		for(new i = 0; i < MAX_BUSINESS; i++){
			if(BusinessData[i][businessID] != 0){
				if(IsPlayerInRangeOfPoint(playerid, 3.0, BusinessData[i][businessExt][0], BusinessData[i][businessExt][1], BusinessData[i][businessExt][2])){
					if(BusinessData[i][businessOwnerSQL] != PlayerData[playerid][pSQLID])return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณไม่ได้เป็นเจ้าของธุรกิจนี้.");

					BusinessData[i][businessOwnerSQL] = 0;
					SaveSQLInt(BusinessData[i][businessID], "businesses", "OwnerSQL", 0);

					GameTextForPlayer(playerid, "You sold your business!", 3000, 5);
					format(string, sizeof(string), "SERVER:{FFFFFF} คุณขายธุรกิจของคุณได้รับเงิน $%s .", MoneyFormat(BusinessData[i][businessMarketPrice] / 2));
					SendClientMessage(playerid, COLOR_ADM, string);

					GiveMoney(playerid, BusinessData[i][businessMarketPrice] / 2);
				}
			}
		}
	}
	return true;
}

