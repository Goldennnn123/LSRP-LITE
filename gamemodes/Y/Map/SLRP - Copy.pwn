CMD:givedrug(playerid, params[])
{
	new playerb, slot, quantity, bslot, string[128];

	if (sscanf(params, "udd", playerb, slot, quantity))
		return SendClientMessage(playerid, COLOR_LIGHTRED, "การใช้: /givedrug [ไอดีผู้เล่น/ชื่อบางส่วน] [drug slot] [quantity]");

	if (!IsPlayerConnected(playerb))
		return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} You specified an invalid player.");

	if (!pLoggedIn[playerb])
		return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} You specified a player that isn't logged in.");

	if(!GetDistanceBetweenPlayers(playerid, playerb, 5.0))
		return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} You aren't close enough to that player.");

	if (slot < 1 || slot > 24)
		return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} You specified an invalid drug slot.");

	if (!DrugData[playerid][drugType][slot])
		return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} You don't have any drugs in that slot.");

	if(quantity < 1)
		return SendClientMessage(playerid, COLOR_ADM, "You specified an invalid quantity. (Greater than 0)");

	if (quantity > DrugData[playerid][drugQuanity][slot])
		return SendClientMessage(playerid, COLOR_ADM, "You don't have the quantity specified in that slot.");

	if (bslot == -1){
		format (string, sizeof(string), "%s can't hold anymore drug packages.", ReturnName(playerb, 0));
		SendClientMessage(playerid, COLOR_ADM, string);
		return true;
	}

	if(playerb == playerid)
		return SendClientMessage(playerid, COLOR_ADM, "You can't give yourselve drugs.");

	bslot = ReturnDrugSlot(playerb);

	WriteLog("drug_logs/givedrug.txt", "[%s] %s gave %s (%d) grams of %s.", ReturnDate(), ReturnName(playerid, 1), ReturnName(playerb, 1),
		quantity, ReturnDrugsName(DrugData[playerid][drugType][slot]));

	DrugData[playerb][drugType][bslot] = DrugData[playerid][drugType][slot];
	DrugData[playerb][drugQuanity][bslot] = quantity;

	format(string, sizeof(string), "Gave %s (%i) to %s", ReturnDrugsName(DrugData[playerid][drugType][slot]), DrugData[playerid][drugQuanity][slot], ReturnName(playerb));
	LogPlayerAction(playerid, string);

	format(string, sizeof(string), "Received %s (%i) from %s", ReturnDrugsName(DrugData[playerid][drugType][slot]), DrugData[playerid][drugQuanity][slot], ReturnName(playerid));
	LogPlayerAction(playerb, string);

	format (string, sizeof(string), "* %s has given %s %s.", ReturnName(playerid, 0), ReturnName(playerb, 0), ReturnDrugsName(DrugData[playerid][drugType][slot]));
	SendClientMessage(playerid, COLOR_EMOTE, string);
	SetPlayerChatBubble(playerid, string, COLOR_EMOTE, 20.0, 3000);

	SendClientMessage(playerid, COLOR_YELLOW, "You've given this person your drug.");
	SendClientMessage(playerb, COLOR_YELLOW, "You've been given a drug..");

	new assurance = DrugData[playerid][drugQuanity][slot] - quantity;
	if (assurance == 0)
	{
		DrugData[playerid][drugType][slot] = 0;
	}

	DrugData[playerid][drugQuanity][slot]-= quantity;
	return true;
}

