	new secret_Word[128], accountIP[20];

	cache_get_field_content(0, "SecretWord", secret_Word, m_Handle, 128);
	cache_get_field_content(0, "IP", accountIP, m_Handle, 20);

	if(isnull(secret_Word))
	{
