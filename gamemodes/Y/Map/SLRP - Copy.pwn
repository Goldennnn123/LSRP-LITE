		count_str[90];

	TruckerJobExitCount[playerid]--;

	format(count_str, 90, "TIME LEFT: %i Seconds", TruckerJobExitCount[playerid]);
	PlayerTextDrawSetString(playerid, TruckerWarning_PTD[playerid][1], count_str);

