		CreateDynamicObject(PlayerAddingModel[playerid], x, y, z, rx, ry, rz, GetPlayerVirtualWorld(playerid), GetPlayerInterior(playerid));

	ObjectData[id][ObjectDBID] = cache_insert_id();
