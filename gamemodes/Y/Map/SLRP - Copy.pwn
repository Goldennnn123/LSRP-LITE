		}

		if(IsTruckerVehicle(vehicleid) && StartedTruckerMission[playerid])
		{
			SendClientMessage(playerid, COLOR_WHITE, "ข้อมูล: คุณสามารถใช้ได้/endmission เพื่อหยุดและรวบรวมรายได้ของคุณ.");
