public RentalCheck()
{
	new string[128];
	for(new i = 1, j = GetVehiclePoolSize(); i <= j; i++)
	{
    if (IsValidVehicle(i))
    	{
        if (!IsVehicleOccupied(i))
   		RentalData[i][rentalcarRented] = false;
		RentalData[i][rentalcarRentedBy] = 0;
    	}
	}
	format(string, sizeof(string), "SERVER: ยานพาหนะที่เช่าที่ไม่ว่างได้ถูกยกเลิกแล้ว.");
	SendClientMessageToAll(COLOR_GREY, string);
	return 1;
}

