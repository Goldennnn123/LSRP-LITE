Server:OnProductPurchase(playerid, response, quantity, price)
{
	if(response)
	{
		if(price > PlayerData[playerid][pMoney])
			return ShowTextdrawInfo(playerid, "~r~You can't afford this.");

		GiveMoney(playerid, -price);

		new string[128];

		format(string, sizeof string, "~g~You spent $%s on %s products!", MoneyFormat(price), MoneyFormat(quantity));
		ShowTextdrawInfo(playerid, string);

		FactionData[ PlayerData[playerid][pFaction] ][factionProducts]+= quantity;
		WriteLog("faction_logs/productbuy.txt", "[%s] %s bought %s products for $%s.", ReturnDateLog(), ReturnName(playerid), MoneyFormat(quantity), MoneyFormat(price));
	}
	return 1;
}

//Vehicle HUDs
stock HideHUDForPlayer(playerid)
{
	switch(PlayerData[playerid][pHUD])
	{
		case 0:
		{
			PlayerTextDrawHide(playerid, VehicleFuel[playerid]);
			PlayerTextDrawHide(playerid, VehicleSpeed[playerid]);
		}
		case 1:
		{
			for(new i = 0; i < 4; i++)
				PlayerTextDrawHide(playerid, VehicleHUDSecond[playerid][i]);
		}
	}
	return 1;
}

stock UpdateHUDForPlayer(playerid)
{
	new
		speedString[20],
		fuelString[20],
		Float:fuel,
		vehicleid = GetPlayerVehicleID(playerid);

	if(!IsValidPlayerCar(vehicleid))
		fuel = 100;

	else fuel = CarData[vehicleid][carFuel];

	format(speedString, 20, "%d", GetVehicleSpeedMPH(vehicleid));
	format(fuelString, 20, "%s%.0f%s", (PlayerData[playerid][pHUD] != 0) ? ("") : ("~y~"), fuel, (PlayerData[playerid][pHUD] != 0) ? ("") : ("%"));

	switch(PlayerData[playerid][pHUD])
	{
		case 0:
		{
			PlayerTextDrawSetString(playerid, VehicleFuel[playerid], fuelString);
			PlayerTextDrawSetString(playerid, VehicleSpeed[playerid], speedString);

			PlayerTextDrawShow(playerid, VehicleFuel[playerid]);
			PlayerTextDrawShow(playerid, VehicleSpeed[playerid]);
		}
		case 1:
		{
			PlayerTextDrawSetString(playerid, VehicleHUDSecond[playerid][3], speedString);
			PlayerTextDrawSetString(playerid, VehicleHUDSecond[playerid][1], fuelString);

			for(new i = 0; i < 4; i++)
				PlayerTextDrawShow(playerid, VehicleHUDSecond[playerid][i]);
		}
	}
	return 1;
}

#define MPH_KMH 1.609344

stock GetVehicleSpeedMPH( vehicleid )
{
	// Function: GetVehicleSpeed( vehicleid )

	new
	    Float:x,
	    Float:y,
	    Float:z,
		vel;

	GetVehicleVelocity( vehicleid, x, y, z );

//	vel = floatround( floatsqroot( x*x + y*y + z*z ) * 180 );			// KM/H
	vel = floatround( floatsqroot( x*x + y*y + z*z ) * 180 / MPH_KMH ); // MPH

	return vel;
}
