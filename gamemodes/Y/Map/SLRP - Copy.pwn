stock GetFightstyleName(style_id)
{
	new
		name[32];

	switch(style_id)
	{
		case 0: name = "Default";
		case 4: name = "Normal";
		case 5: name = "Boxing";
		case 6: name = "Kungfu";
		case 7: name = "Kneehead";
		case 15: name = "Grabkick";
		case 16: name = "Elbow";
	}
	return name;
}

