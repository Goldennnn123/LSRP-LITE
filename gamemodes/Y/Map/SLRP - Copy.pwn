//IRC Commands
IRCCMD:asay(botid, channel[], user[], host[], params[])
{
	if (IRC_IsOp(botid, channel, user))
	{
		if(!isnull(params))
		{
			new
				str[128];

			if(strlen(params) > 90)
			{
				format(str, sizeof(str), "**%s on IRC: %.90s", user, params);
				SendAdminMessage(0x6CEFF0FF, str);

				format(str, sizeof(str), "**%s on IRC: ...%s", user, params[90]);
				SendAdminMessage(0x6CEFF0FF, str);
			}
			else
			{
				format(str, sizeof(str), "**%s on IRC: %s", user, params);
				SendAdminMessage(0x6CEFF0FF, str);
			}
		}
		else IRC_Say(botid, user, "You didn't write anything.");
	}
	return 1;
}

IRCCMD:ajail(botid, channel[], user[], host[], params[])
{
	if(IRC_IsOp(botid, channel, user))
	{
		new
			playerid, time, reason[64], str[128], query[256];

		if(sscanf(params, "dds[64]", playerid, time, reason))
		{
			IRC_Say(botid, user, "Correct command: !ajail [playerid] [time in minutes] [reason]");
			return 1;
		}

		if(time < 1)
		{
			IRC_Say(botid, user, "Time cannot be below 1 minute!");
			return 1;
		}

		if(IsPlayerConnected(playerid))
		{
			ClearAnimations(playerid);
			ClearPlayerWeapons(playerid, 0);

			SetPlayerPos(playerid, 2578.929931, 2658.771728, 10.820312);
			SetPlayerVirtualWorld(playerid, 1338);
			SetPlayerInterior(playerid, 0);

			ResetHouseVar(playerid);

			if(StartedTruckerMission[playerid])
			{
				EndTruckerMission(playerid);
			}

			if(InTaxiRide[playerid])
			{
				foreach(new i : Player) if(PlayerData[i][pSQLID] == TaxiDriver[playerid])
				{
					SendClientMessage(i, COLOR_YELLOW, "-> A person in your taxi was admin jailed. You were compensated regardless.");
				}

				EndTaxiFair(playerid);
			}

			if(StartedTaxiJob[playerid] && TaxiFairStarted[playerid])
			{
				foreach(new i : Player) if(TaxiDriver[i] == PlayerData[playerid][pSQLID])
				{
					SendClientMessage(i, COLOR_YELLOW, "-> Your taxi driver was admin jailed. The fair ended.");
					EndTaxiFair(i);
				}

				EndTaxiFairDriver(playerid);
			}

			if(OrderedWeapon[playerid])
				ClearPlayerOrder(playerid);

			if(strlen(reason) > 45)
			{
				SendMessageToAll(COLOR_ADM, "AdmCmd: %s has been admin jailed by SYSTEM for %d minutes, Reason: %.45s", ReturnName(playerid, 1), time, reason);
				SendMessageToAll(COLOR_ADM, "AdmCmd: ...%s", reason[45]);
			}
			else SendMessageToAll(COLOR_ADM, "AdmCmd: %s has been admin jailed by SYSTEM for %d minutes, Reason: %s", ReturnName(playerid, 1), time, reason);

			PlayerData[playerid][pAjailed] = true;
			PlayerData[playerid][pAjailTime] = time * 60;

			ClearPlayerWeapons(playerid, 0);

			for(new i = 1; i < MAX_ADMIN_NOTES; i++)
			{
				if(aNotesData[playerid][i][anote_SQLID] != 0)
				{
					if(aNotesData[playerid][i][anote_active])
					{
						if(strlen(aNotesData[playerid][i][anote_reason]) > 45)
						{
							format(str, sizeof(str), "Player %s has %d active OnAdminAction notes on him, last is: %.45s", ReturnName(playerid, 1), CountAdminNotes(playerid), aNotesData[playerid][i][anote_reason]);
							adminWarn(1, str);

							format(str, sizeof(str), "...%s", aNotesData[playerid][i][anote_reason][45]);
							adminWarn(1, str);
						}
						else
						{
							format(str, sizeof(str), "Player %s has %d active OnAdminAction notes on him, last is: %s", ReturnName(playerid, 1), CountAdminNotes(playerid), aNotesData[playerid][i][anote_reason]);
							adminWarn(1, str);
						}
					}
				}
			}

			mysql_format(m_Handle, query, sizeof(query), "INSERT INTO log_ajail (`Name`, `Reason`, `JailedBy`, `Date`, `Time`, `IP`) VALUES('%e', '%e', 'SYSTEM', '%e', %i, '%e')", ReturnName(playerid, 1), reason, ReturnDateLog(), time, ReturnIP(playerid));
			mysql_pquery(m_Handle, query);

			format(str, sizeof(str), "AdmCmd: %s was admin jailed by %s (IRC) for %d minutes, Reason: %s", ReturnName(playerid, 1), user, time, reason);
			IRC_GroupSay(groupReport, IRC_CHANNEL_EX, str);

			WriteLog("admin_logs/punishments/ajail_log.txt", "[%s] %s (IRC) has admin jailed %s for %d minutes, Reason: %s", ReturnDate(), user, ReturnName(playerid, 1), time, reason);
		}
	}
	return 1;
}

