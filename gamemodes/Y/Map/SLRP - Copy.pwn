		return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณไม่ได้อยู่ในธนาคาร.");

	if(sscanf(params, "d", deposit))return SendClientMessage(playerid, COLOR_LIGHTRED, "การใช้: /bank [จํานวนเงิน]");
