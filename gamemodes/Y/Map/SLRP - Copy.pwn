CMD:department(playerid, params[])
{
	if(!pLoggedIn[playerid])return true;
	new string[128];
	new myfaction = PlayerData[playerid][pFaction];

	if (!PlayerData[playerid][pFaction])return SendClientMessage(playerid, COLOR_ADM, "ERROR:{FFFFFF} คุณไม่มีแฟคชั่น.");
