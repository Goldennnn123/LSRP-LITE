		SendClientMessage(playerid, COLOR_ADM, "คุณปิดใช้งาน player tags. /tognick เพื่อเปิด.");
		PlayerData[playerid][pNicksToggled] = 1;
	}
	else
	{
		foreach(new i : Player)
		{
			if(PlayerData[i][pMasked])
				continue;

			ShowPlayerNameTagForPlayer(playerid, i, 1);
		}

		SendClientMessage(playerid, COLOR_ADM, "คุณเปิดชื่อผู้เล่น.");
		PlayerData[playerid][pNicksToggled] = 0;
	}
	return 1;
}

