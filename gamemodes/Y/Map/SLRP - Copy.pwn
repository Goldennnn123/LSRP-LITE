CMD:fine(playerid, params[])
{
	new
		playerb, amount, reason[128], query[256];

	if (sscanf(params, "uds[128]", playerb, amount, reason))
		return SendClientMessage(playerid, COLOR_ADM, "USAGE: /fine [ไอดีผู้เล่น/ชื่อบางส่วน] [จํานวนเงิน] [ข้อหา]");

	if(!IsPlayerConnected(playerb))
		return SendClientMessage(playerid, COLOR_ADM, "ERROR: คุณระบุผู้เล่นที่ไม่ถูกต้อง.");

	if(!pLoggedIn[playerb])
		return SendClientMessage(playerid, COLOR_ADM, "ERROR: ผู้เล่นนั้นไม่ได้เข้าสู่ระบบ.");

	if(!GetDistanceBetweenPlayers(playerid, playerb, 4.5))
		return SendClientMessage(playerid, COLOR_ADM, "ERROR: คุณไม่ได้อยู่ใกล้ผู้เล่นคนนั้น.");

	if(strlen(reason) < 3)
		return SendClientMessage(playerid, COLOR_ADM, "ERROR: คุณต้องระบุเหตุผล.");

	if(amount < 1)
		return SendClientMessage(playerid, COLOR_ADM, "ERROR: คุณไม่สามารถออกเงินจำนวนน้อยกว่าหนึ่งดอลลาร์ได้.");

	new
		id = -1;

	for (new i = 0; i < 30; i++)
	{
		if(Fines[playerb][i][Fine_ID])
			continue;

		id = i;
		break;
	}

	if(id == -1)
		return SendClientMessage(playerid, COLOR_ADM, "ผู้เล่นมีใบสั่งอยู่ 30 ใบและไม่สามารถเพิ่มได้อีก...");

	mysql_format(m_Handle, query, sizeof(query), "INSERT INTO player_fines (player_dbid, issuer_name, fine_amount, fine_reason, fine_date) VALUES(%i, '%e', %i, '%e', '%e')", PlayerData[playerb][pSQLID], ReturnName(playerid, 1), amount, reason, ReturnDate());
	mysql_tquery(m_Handle, query, "OnFineIssued", "iisi", playerid, playerb, reason, amount);
	return 1;
}

