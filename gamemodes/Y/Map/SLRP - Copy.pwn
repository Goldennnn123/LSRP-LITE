CMD:ouninvite(playerid, params[])
{
	if(!PlayerData[playerid][pFaction])
		return SendClientMessage(playerid, COLOR_ADM, "ERROR:.คุณไม่ได้อยู่ในกลุ่ม");

	if(PlayerData[playerid][pFactionRank] > FactionData[PlayerData[playerid][pFaction]][factionEditrank])
		return SendClientMessage(playerid, COLOR_ADM, "ERROR: คุณไม่มีสิทธิ์เข้าถึงคำสั่งนี้.");

	if(isnull(params))
		return SendClientMessage(playerid, COLOR_ADM, "USAGE: /ouninvite [character name]");

	if(!IsValidRoleplayName(params))
		return SendClientMessage(playerid, COLOR_ADM, ":กำลังคิด: ชื่อเป็นชื่อบทบาทสมมุติที่ไม่ถูกต้อง ตัวพิมพ์เล็ก - ใหญ่ FYI.");

	if(!ReturnSQLFromName(params))
		return SendClientMessage(playerid, COLOR_ADM, "บุคคลนั้นไม่มีอยู่ในฐานข้อมูล.");

	foreach(new i : Player)
	{
		if(!strcmp(ReturnName(i), params, true))
		{
			SendClientMessage(playerid, COLOR_ADM, "ผู้เล่นออนไลน์.");
			return 1;
		}
	}

	new query[230];
	new faction_id, rank_id;

	mysql_format(m_Handle, query, sizeof query, "SELECT Faction, FactionRank FROM players WHERE id = %i", ReturnSQLFromName(params));
	new Cache:cache = mysql_query(m_Handle, query);

	faction_id = cache_get_field_content_int(0, "Faction", m_Handle);
	rank_id = 	 cache_get_field_content_int(0, "FactionRank", m_Handle);

	if(faction_id != PlayerData[playerid][pFaction])
	{
		SendClientMessage(playerid, COLOR_ADM, "ผู้เล่นคนนั้นไม่ได้อยู่ในกลุ่มของคุณ.");
		cache_delete(cache);
		return 1;
	}

	if(rank_id < PlayerData[playerid][pFactionRank])
	{
		SendClientMessage(playerid, COLOR_ADM, "ผู้เล่นนั้นมีอันดับเกินกว่าคุณ.");
		cache_delete(cache);
		return 1;
	}

	format(query, sizeof query, "คุณแน่ใจหรือไม่ว่าต้องการยกเลิกการเชิญ %s จาก Faction ของคุณ?", params);
	ConfirmDialog(playerid, "Confirmation", query, "OnOfflineUninvite", ReturnSQLFromName(params));

	cache_delete(cache);
	return true;
}

