	if(AMMO_ANTI_CHEAT)
	{
		foreach(new i : Player)
		{
			if(!pLoggedIn[i])
				continue;

			if(NetStats_GetConnectedTime(i) - PlayerConnectionTick[i] < 300)
				continue;

			if(gettime() - PlayerData[i][pPauseWepAC] < 4 && PlayerData[i][pPauseWepAC] != 0)
				continue;

			if(ACPausedForPlayer[i])
				continue;

			new string[128]; new weapon_id, ammo;
			new LOCAL_CHEAT_DETECTED;
			new WEAPON_FOUND;
			new WEAPON_AMMO;
			new WEAPON_INDEX;

			for(new w = 0; w < 13; w++)
			{
				GetPlayerWeaponData(i, w, weapon_id, ammo);

				if(weapon_id > 0 && ammo > 0)
				{
					if(weapon_id == 41)
						continue;

					WEAPON_INDEX = GetWeaponIndex(weapon_id);

					if(Weapon[i][WEAPON_INDEX][eWeaponID] == weapon_id && ammo - 1 > Weapon[i][WEAPON_INDEX][eWeaponAmmo])
					{
						LOCAL_CHEAT_DETECTED = 1;
						WEAPON_FOUND = weapon_id;
						WEAPON_AMMO = ammo;

						if(!TriggeredAmmo[i])
							TriggeredAmmo[i] = gettime();
						break;
					}
				}
			}

			if(LOCAL_CHEAT_DETECTED)
			{
				new online;
				if(IsPlayerConnected(i) && PlayerData[i][pAdmin] > 0)
    				{
        				online++;
        				if(online == 0)
						{
		    				new query[400], reason[256];
							mysql_format(m_Handle, query, sizeof(query), "INSERT INTO bans (`Name`, `Reason`, `BannedBy`, `Date`, `IP`, `HWID`) VALUES('%e', 'Ammo Hacks', 'Anti Cheat', '%e', '%e', '%e')", ReturnName(i), ReturnDate(), ReturnIP(i), ReturnGPCI(i));
							mysql_pquery(m_Handle, query);

							mysql_format(m_Handle, query, sizeof(query), "INSERT INTO log_bans (Name, Reason, BannedBy, Date, IP) VALUES('%e', 'Ammo Hacks', 'Anti Cheat', '%e', '%e')", ReturnName(i), ReturnDateLog(), ReturnIP(i));
							mysql_pquery(m_Handle, query);

							format(reason, sizeof(reason), "%s was banned by ANTICHEAT, reason: Triggering ammo anticheat.", ReturnName(i));
							SendClientMessageToAll(COLOR_ADM, reason);

							KickEx(i);
						}
						else {

							if(gettime() - TriggeredAmmo[i] > 5)
							{
							format(string, sizeof(string), "%s(ID: %i) may be AMMO HACKING. Weapon:%s, Ammo: %d, Real Ammo:%d", ReturnName(i), i, ReturnWeaponName(WEAPON_FOUND), WEAPON_AMMO, Weapon[i][WEAPON_INDEX][eWeaponAmmo]);
							adminWarn(1, string);

							format(string, sizeof(string), "4[WARNING] 1%s(ID: %i) may be AMMO HACKING. Weapon:%s, Ammo: %d, Real Ammo:%d", ReturnName(i), i, ReturnWeaponName(WEAPON_FOUND), WEAPON_AMMO, Weapon[i][WEAPON_INDEX][eWeaponAmmo]);
							IRC_GroupSay(groupAnti, IRC_CHANNEL_ANTI, string);

							TriggeredAmmo[i] = gettime();
							}
						}
				}
			}
		}
	}
}

